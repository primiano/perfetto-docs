{
  "name": "perfetto-dev",
  "version": "1.0.0",
  "description": "Perfetto Site",
  "repository": "https://perfetto.dev",
  "main": "main.js",
  "author": "Perfetto Team",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^3.5.0",
    "ejs": "^3.0.1",
    "fs-extra": "^9.0.0",
    "highlight.js": "^9.18.1",
    "jsdoc-api": "^5.0.4",
    "marked": "^0.8.0",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "protobufjs": "^6.9.0",
    "yargs": "^15.1.0"
  },
  "scripts": {
    "clean": "gn clean ../../out/perfetto.dev",
    "setup": "ln -fns ../../out/perfetto.dev/site dist",
    "serve": "npm run setup; dev_appserver.py .",
    "build": "npm run setup; gn gen --args='' ../../out/perfetto.dev; ninja -C ../../out/perfetto.dev site",
    "watch": "onchange -v '../../docs/**' '**' -e 'dist' -- npm run build",
    "start": "npm run build; concurrently \"npm run serve\" \"npm run watch\""
  }
}
